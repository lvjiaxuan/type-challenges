    "typescript": "^4.9.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --cache"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:@lvjiaxuan/recommended"
